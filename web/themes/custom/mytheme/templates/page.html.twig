<header aria-label="Site header" class="header" id="header" role="banner">
  {{ page.header }}
  {{ page.primary_menu }}
  {{ page.secondary_menu }}
</header>
<section class="navigation" id="navigation" role="menus" >
  {{ page.navigation }}
</section>
<section class="slide" id="slide" role="sliders">
  {{ page.slide }}
<section class="main" id="main">
  <aside class="left--sidebar" id="sidebar" role="complementary">
    {{ page.left_sidebar }}
  </aside>
  <main aria-label="Site main content" class="content" id="content" role="main">
    {{ page.content }}
  </main>

  <aside class="right--sidebar" id="sidebar2" role="complementary">
    {{ page.right_sidebar }}
  </aside>
</section>
<footer aria-label="Site footer" class="footer" id="footer" role="contentinfo">
  <div class="footer--bottom">
    {{ page.footer }}
  </div>
</footer>
